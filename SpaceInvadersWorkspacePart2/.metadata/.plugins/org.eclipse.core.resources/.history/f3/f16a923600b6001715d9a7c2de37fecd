#include "draw_tank.h"

static uint16_t tankPosition; //Variable to keep track of tank position

void setTankPosition(uint16_t val){ //Set the position of the tank
	tankPosition = val;
}

uint16_t getTankPosition(){ //get the position of the tank
	return tankPosition;
}
